{
  "name": "@dcl/asset-packs",
  "description": "Decentraland Asset Packs - curated collections of 3D assets for Decentraland scenes",
  "version": "0.1.0",
  "author": {
    "email": "hello@decentraland.org",
    "name": "Decentraland Foundation",
    "url": "https://decentraland.org"
  },
  "bugs": "https://github.com/decentraland/creator-hub/issues",
  "dependencies": {
    "@types/glob": "^8.1.0",
    "glob": "^11.0.0",
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.405.0",
    "@aws-sdk/lib-storage": "^3.405.0",
    "@dcl/hashing": "^3.0.4",
    "@dcl/js-runtime": "7.15.2",
    "@dcl/react-ecs": "7.15.2",
    "@dcl/sdk-commands": "7.15.2",
    "@gltf-transform/core": "^4.3.0",
    "@types/mime-types": "^2.1.1",
    "@types/node-fetch": "^2.6.4",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "fp-future": "^1.0.1",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "p-queue": "^7.4.1",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "serve": "^14.2.1",
    "serve-handler": "^6.1.5",
    "ts-node": "^10.9.1",
    "typescript": "5.5.3"
  },
  "files": [
    "dist",
    "bin",
    "catalog.json"
  ],
  "homepage": "https://github.com/decentraland/creator-hub/tree/main/packages/asset-packs#readme",
  "keywords": [
    "3d",
    "assets",
    "decentraland",
    "sdk7",
    "smart-items"
  ],
  "license": "ISC",
  "main": "dist/definitions.js",
  "peerDependencies": {
    "@dcl/ecs": "7.15.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/creator-hub.git",
    "directory": "packages/asset-packs"
  },
  "scripts": {
    "add-bounding-boxes": "ts-node --project tsconfig.scripts.json ./scripts/add-bounding-boxes.ts",
    "build": "npm run build:js && npm run build:lib && npm run build:catalog",
    "build:catalog": "ts-node --project tsconfig.scripts.json ./scripts/catalog.ts",
    "build:js": "sdk-commands build",
    "build:lib": "tsc --project tsconfig.lib.json",
    "download": "ts-node --project tsconfig.scripts.json ./scripts/download.ts",
    "migrate": "ts-node --project tsconfig.scripts.json ./scripts/migrate.ts",
    "start": "concurrently \"npm run start:js\" \"npm run start:lib\" \"npm run start:catalog\"",
    "start:catalog": "nodemon --watch packs -x \"npm run build:catalog\"",
    "start:dev": "concurrently \"npm run start:js -- --data-layer\" \"npm run start:lib\" \"npm run start:catalog\"",
    "start:js": "sdk-commands start --no-browser --port 8001",
    "start:lib": "npm run build:lib -- --watch",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "upload": "ts-node --project tsconfig.scripts.json ./scripts/upload.ts",
    "validate": "ts-node --project tsconfig.scripts.json ./scripts/validate.ts"
  },
  "typings": "dist/definitions.d.ts"
}
